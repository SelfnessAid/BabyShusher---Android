<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="babyshusher.babyshusher.com.babyshusher">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OptionActivity" />
        <activity android:name=".QuickStartActivity" />
        <activity android:name=".FAQActivity" />
        <activity android:name=".TimerActivity" />
        <activity android:name=".EqualizerActivity" />
        <activity android:name=".VideoAcitivity"></activity>
    </application>

</manifest>